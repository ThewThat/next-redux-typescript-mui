{
  "name": "next-typescript",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index",
  "typings": "dist/index",
  "scripts": {
    "dev": "concurrently \"npm run lint-js:watch\" \"npm run lint-md:watch\" \"next dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint-md": "remark .",
    "lint-md:watch": "remark --watch .",
    "lint-js": "eslint -c ./.eslintrc --ignore-path ./.eslintignore ./src",
    "lint-js:watch": "esw -w -c ./.eslintrc --ignore-path ./.eslintignore",
    "prepare": "husky install",
    "pre-commit": "next lint"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/material": "^5.9.1",
    "@next/eslint-plugin-next": "^12.2.3",
    "@reduxjs/toolkit": "^1.8.3",
    "@types/browserify": "latest",
    "browserify": "latest",
    "concurrently": "^7.3.0",
    "eslint-config-prettier": "^8.5.0",
    "next": "12.2.3",
    "next-redux-wrapper": "^7.0.5",
    "prettier": "^2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.2",
    "react-video-thumbnail": "^0.1.3",
    "redux": "^4.2.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@types/next-redux-wrapper": "^3.0.0",
    "@types/node": "18.6.1",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/react-redux": "^7.1.24",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "eslint": "^8.22.0",
    "eslint-config-next": "12.2.3",
    "husky": "^8.0.1",
    "typescript": "^4.7.4"
  }
}
